{"legislative_session": "2019", "identifier": "HB 1640", "title": "Health carriers; services provided by nurse practitioners.", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["Health", "Insurance", "Professions and Occupations"], "abstracts": [{"note": "summary", "abstract": "Requires health insurers and health services plan providers whose policies or contracts cover services that may be legally performed by licensed nurse practitioners to provide equal coverage for such services when rendered by a licensed nurse practitioner. The bill contains an enactment that exempts the measure from the requirement that the Health Insurance Reform Commission review any legislative measure containing a mandated health insurance benefit or provider. The bill has a delayed effective date of October 1, 2019.", "date": ""}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Prefiled and ordered printed; offered 01/09/19 19100868D", "date": "2018-11-26", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["introduction"], "related_entities": [], "extras": {}}, {"description": "Referred to Committee on Commerce and Labor", "date": "2018-11-26", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [], "extras": {}}, {"description": "Read first time", "date": "2019-02-03", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": [], "extras": {}}, {"description": "Read second time", "date": "2019-02-04", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-2"], "related_entities": [], "extras": {}}, {"description": "Committee amendment agreed to", "date": "2019-02-04", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": [], "extras": {}}, {"description": "Engrossed by House as amended HB1640E", "date": "20