{"legislative_session": "2019", "identifier": "HB 1639", "title": "Medical Assistance Services, Department of; payment for certain services to hospice patients.", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["Health"], "abstracts": [{"note": "summary", "abstract": "Directs the Department of Medical Assistance Services to implement a process for direct payment of nursing facility or ICF/MR services for hospice patients receiving services in a nursing facility or ICF/MR, if such patient would be eligible for medical assistance for nursing facility or ICF/MR services had he not elected to receive hospice care. The bill provides that payments made to a nursing facility or ICF/MR shall be the full amount that would be paid to the nursing facility or ICF/MR, if the individual was not receiving hospice services, and shall not reflect any discount to such rates.", "date": ""}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Prefiled and ordered printed; offered 01/09/19 19100544D", "date": "2018-11-21", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["introduction"], "related_entities": [], "extras": {}}, {"description": "Referred to Committee on Appropriations", "date": "2018-11-21", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [], "extras": {}}, {"description": "Assigned App. sub: Health & Human Resources", "date": "2019-01-11", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [], "extras": {}}, {"description": "Read first time", "date": "2019-02-01", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": [], "extras": {}}, {"description": "Read second time and engrossed", "date": "2019-02-04", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-2"]