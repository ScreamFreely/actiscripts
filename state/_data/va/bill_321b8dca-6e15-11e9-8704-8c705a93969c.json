{"legislative_session": "2019", "identifier": "HB 1674", "title": "Abuse and neglect of incapacitated adults; informed consent.", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["Crimes and Offenses Generally", "Persons with Disabilities"], "abstracts": [{"note": "summary", "abstract": "Clarifies, for the purposes of the exemptions to abuse and neglect of incapacitated adults, that the informed consent or a declaration of the incapacitated person must have been given when such person was not incapacitated and that any wishes of the incapacitated person relied upon must have been made known when such person was not incapacitated. The bill provides that its provisions are declaratory of existing law.", "date": ""}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Prefiled and ordered printed; offered 01/09/19 19100716D", "date": "2018-12-03", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["introduction"], "related_entities": [], "extras": {}}, {"description": "Referred to Committee for Courts of Justice", "date": "2018-12-03", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [], "extras": {}}, {"description": "Assigned Courts sub: Subcommittee #1", "date": "2019-01-10", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [], "extras": {}}, {"description": "Read first time", "date": "2019-01-23", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": [], "extras": {}}, {"description": "Read second time", "date": "2019-01-24", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-2"], "related_entities": [], "extras": {}}, {"description": "Committee amendments agreed to", "date": "2019-01-24", "organization_id": "~{\"classification\": \"lower\"}", "classifica