        <span class="sr-only">new text end</span>
        <span class="sr-only">new text begin</span>
        <ins style="text-decoration: underline"> and </ins>
        <span class="sr-only">new text end</span>
        <span class="sr-only">new text begin</span>
        <ins style="text-decoration: underline">Minnesota <br/><span id="pl.3.3" class="pl"> </span>Rules, part 6256.0500, subparts 2, 2a, 2b, 4, 5, 6, 7, and 8,</ins>
        <span class="sr-only">new text end</span>
        <span class="sr-only">new text begin</span>
        <ins style="text-decoration: underline"> are repealed.<br/></ins>
        <span class="sr-only">new text end</span>
