    <script type = "text/javascript">

        $(document).ready(function () {

            var currentURL = window.location.href; //gets url of clicked bill
            var resultArray = sessionStorage.result.split(','); //stores urls of results from search in array
            var resultLength = resultArray.length; //variable to store array length
            var resultIndex = $.inArray(currentURL, resultArray); //> -1 //Looks for current url in the array
            var regexPattern = /\&f=(.*)\&s/; //expression to extract bill name



            if (resultLength > 1) //Only if there is more than one element
            {
                $("div.leg-content").prepend("<div id='bill_button'><div>Search Results:</div> </div>"); //add div element to le_OfficeNav
                getPrev(resultIndex, resultArray, regexPattern);
                getResults(resultIndex, resultArray, resultLength, regexPattern);
                getNext(resultIndex, resultArray, resultLength, regexPattern);
            }
        });
            function getResults (index, array, length, pattern)
            {
                var select = document.createElement('select'); //creates a drop down menu for search results
                document.getElementById("bill_button").appendChild(select);
                $(select).addClass('form-control form-control-sm');
                $(select).css({"display": "inline",
                               "padding": "0",
                               "width": "auto",
                               "margin-left": "5px"
                               }); 
                $(select).attr(   //assign an id to the select element
                    {
                        'id': 'bill_select'
                    });

                //if there is an element in list is selected, go to the url
                $('#bill_select').change(function(){

                    window.location = $("#bill_select option:selected").val();
                });

                for (var i=0; i<length; i++) //populates the dropdown menu
                {
                    var bill_name = pattern.exec(array[i])[1];
                    var bill_url = array[i];
                    var el = document.createElement("option");
                    el.textContent = bill_name;
                    el.value =bill_url;
                    if (i == index) {
                        el.selected = array[index]; //the current bill is the selected option
                    }
                    select.appendChild(el);
                }
            }

            function getPrev(index,array, pattern){

                var prev = document.createElement('a');
                if (index > 0) //if not the first element
                {
                    prev.href = array[index -1];
                    var prevName = pattern.exec(array[index -1])[1];
                    document.getElementById("bill_button").appendChild(prev);
                }

                var prevText = document.createTextNode(prevName);
                prev.appendChild(prevText);
                $(prev).addClass("previous");
                prev.title = prevName;
                //console.log(prevName);

            }

            function getNext(index,array,length)
            {
                var next = document.createElement('a');
                //console.log(arr.length);
                if (index < (length-1)) //if not the last element
                {
                    next.href = array[index+1];
                    var nextName = array[index +1].split('&')[1].split('=')[1];
                    document.getElementById("bill_button").appendChild(next);
                }

                var nextText = document.createTextNode(nextName);
                next.appendChild(nextText);
                $(next).addClass("next");
                next.Title = nextName;
                //console.log(nextName);
            }

            //oo refactor TO DO
