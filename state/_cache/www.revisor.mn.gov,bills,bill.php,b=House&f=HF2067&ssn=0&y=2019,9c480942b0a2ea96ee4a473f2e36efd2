                var resultLength = resultArray.length; //variable to store array length
                var resultIndex = $.inArray(currentURL, resultArray); //> -1 //Looks for current url in the array
                var regexPattern = /\&f=(.*)\&s/; //expression to extract bill name

                //get search results;
               this.getResult = function (arr){

                  arr = sessionStorage.results.split(',');
                   return arr;
                }

                this.getCurrent = function(arr, currentURL){

                    var found = $.inArray(currentURL,arr);
                    return found;
                }

                this.getNext(arr, found, ){

                    var pattern =
                }
            }*/


