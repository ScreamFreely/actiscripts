<div id="block-cga-committees-committee-activity" class="block block-cga-committees last odd">
  <div class="block-container">
    <div class="block-title-container">
                    <h2 class="block__title block-title">Committee Activity</h2>
                </div>
    <div class="block-content">
      
  <ul class="accordion">
            
      <li
        class="accordion-item is-active"
        data-accordion-item
        data-meeting-uuid="S_HHS_2019A_20190425_134128" >

        <a class="accordion-title" tabindex="0">
          <h5>
            <div class="date-wrapper">
              <div class="field field-name-field-date field-type-datetime field-label-hidden">
    <div class="field-items">
          <div class="field-item even"><span class="date-display-single" property="dc:date" datatype="xsd:dateTime" content="2019-04-25T13:41:28-06:00">04/25/2019</span></div>
      </div>
</div>
              <span class="count">(9)</span>
            </div>
          </h5>
        </a>
        <div class="accordion-content" data-tab-content>
          <div class="activity-meta">
            <div class="description-wrapper">
                          </div>
