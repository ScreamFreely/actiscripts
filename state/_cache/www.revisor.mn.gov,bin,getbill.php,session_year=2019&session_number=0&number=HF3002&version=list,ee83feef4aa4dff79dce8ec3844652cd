    <var class="ln">5.7</var>
    <var class="ln">5.8</var>
    <var class="ln">5.9</var>
    <var class="ln">5.10</var>
    <var class="ln">5.11</var>
    <var class="ln">5.12</var>
    <var class="ln">5.13</var>
