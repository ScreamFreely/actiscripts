                <td/>
                <td style="text-align: right;"><span id="pl.82.21" class="pl"> </span>$<br/></td>
                <td style="text-align: right;">27,158,000<br/></td>
                <td style="text-align: right;">.....<br/></td>
                <td style="text-align: right;">2018<br/></td>
