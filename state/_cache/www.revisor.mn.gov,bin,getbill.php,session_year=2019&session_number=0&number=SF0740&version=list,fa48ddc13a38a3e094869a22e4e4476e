    <var class="ln">4.25</var>
    <var class="ln">4.26</var>
    <var class="ln">4.27</var>
    <var class="ln">4.28</var>
    <var class="ln">4.29</var>
    <var class="ln">4.30</var>
    <var class="ln">4.31</var>
    <var class="ln">4.32</var>
    <var class="ln">4.33</var>
    <var class="ln">4.34</var>
    <var class="ln">5.1</var>
    <var class="ln">5.2</var>
    <var class="ln">5.3</var>
    <var class="ln">5.4</var>
    <var class="ln">5.5</var>
    <var class="ln">5.6</var>
    <var class="ln">5.7</var>
    <var class="ln">5.8</var>
