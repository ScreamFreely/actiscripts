    <div class="article" id="bill.2.0.0">
      <h1 class="article_no"><span id="pl.38.21" class="pl"> </span>ARTICLE 2<br/></h1>
      <h1 class="article_header"><span id="pl.38.22" class="pl"> </span>ENVIRONMENT AND NATURAL RESOURCES TRUST FUND<br/></h1>
      <div class="bill_section appropriations" id="bill.2.1.0">
        <table>
          <col style="width:33.0769230769231%"/>
          <col style="width:3.846153846153846%"/>
