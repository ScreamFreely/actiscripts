	<script>
	var validator = new FormValidator('memberForm', [{
		name: 'sender',
		display: 'E-mail Address',
		rules: 'required|valid_email'
	}, {
		name: 'lname',
		display: 'Name',
		rules: 'required'
	}, {
		name: 'address1',
		display: 'Address',
		rules: 'required'
	}, {
		name: 'city',
		display: 'City',
		rules: 'required'
	}, {
		name: 'state',
		display: 'State',
		rules: 'required'
	}, {
		name: 'zipcode',
		display: 'ZIP code',
		rules: 'required'
	}], function(errors, event) {
		if (errors.length > 0) {
			var errorString = 'The following fields must be corrected:\n\n';
			for (var i = 0, errorLength = errors.length; i < errorLength; i++) {
				errorString += errors[i].message + '\n';
			}
			alert(errorString);
		}
	});
	</script>
