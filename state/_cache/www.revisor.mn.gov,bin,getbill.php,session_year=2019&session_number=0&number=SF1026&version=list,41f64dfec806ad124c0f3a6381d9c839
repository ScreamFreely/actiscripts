    <var class="ln">4.31</var>
    <var class="ln">4.32</var>
    <var class="ln">5.1</var>
    <var class="ln">5.2</var>
    <var class="ln">5.3</var>
    <var class="ln">5.4</var>
    <var class="ln">5.5</var>
    <var class="ln">5.6</var>
    <var class="ln">5.7</var>
    <var class="ln">5.8</var>
    <var class="ln">5.9</var>
    <var class="ln">5.10</var>
    <var class="ln">5.11</var>
    <var class="ln">5.12</var>
    <var class="ln">5.13</var>
    <var class="ln">5.14</var>
    <var class="ln">5.15</var>
    <var class="ln">5.16</var>
    <var class="ln">5.17</var>
    <var class="ln">5.18</var>
    <var class="ln">5.19</var>
    <var class="ln">5.20</var>
    <var class="ln">5.21</var>
    <var class="ln">5.22</var>
